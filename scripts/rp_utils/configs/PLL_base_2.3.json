{
  "seed": 1,
  "network": "transformer",
  "batch_size": 128,
  "lr_init": 0.003,
  "warmup_steps": 30000,
  "stopping_criterion": "num_played_steps",
  "lr_decay_rate": 0.99,
  "value_loss_weight": 0.5,
  "replay_buffer_size": 5000,
  "transformer_hidden_size": 128,
  "td_steps": 25,
  "num_unroll_steps": 7,
  "append": "_PLL_v2.2",
  "softmax_limits": [0.2, 0.4, 0.6, 0.8, 1],
  "mvc_softmax_temps": [1.5, 1.25, 1.25, 1.0, 1.0],
  "use_softmax": true,
  "num_simulations": 3,
  "fc_representation_layers": [32],
  "mlp_head_layers": [64, 32],
  "max_seq_length": 75,

  "expansion_strategy": "deep_new",
  "subtree_layers": 2,
  "action_selection": "mvc",
  "PUCT_C": 2.5,
  "PUCT_variant": "mvc",
  "mvc_beta": 0.02,
  "policy_target_type": "mvc",
  "test_ucb": false,

  "training_steps": 300000,
  "num_workers": 1
}